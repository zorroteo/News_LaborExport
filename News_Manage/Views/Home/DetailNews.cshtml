@model NewsProject.Entitys.tblNew
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="/Uploads/images/images/presets/preset1/logo-icon.png">
    <!--title-->
    <title>Tư vấn du học, suất khẩu lao động và xin Visa</title>
    @Styles.Render("~/Content/bundlesnews")

    <!--Google Fonts-->
    <link href='https://fonts.googleapis.com/css?family=Signika+Negative:400,300,600,700' rel='stylesheet' type='text/css'>

    @*fa icon*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="shortcut icon" href="images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Uploads/images/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Uploads/images/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Uploads/images/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/Uploads/images/images/ico/apple-touch-icon-57-precomposed.png">
</head><!--/head-->
<body>
    <div id="main-wrapper" class="page">

        <header id="navigation">
            <div class="navbar sticky-nav" role="banner">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="index.html">
                            <img class="main-logo img-responsive" src="~/Uploads/images/images/presets/preset1/logo.png" alt="logo">
                        </a>
                    </div>
                    @Html.Partial("../menu/newsnavbar")
                    @*<nav id="mainmenu" class="navbar-left collapse navbar-collapse">
                            <ul class="nav navbar-nav">
                                <li class="home">
                                    <a href="/home/index">Home</a>
                                </li>
                                <li class="business dropdown">
                                    <a href="/home/group?group=@News_Manage.Common.Country.Japan&page=1">Du học Nhật</a>
                                </li>
                                <li class="politics dropdown">
                                    <a href="/home/group?group=@News_Manage.Common.Country.Korea">Du học Hàn</a>
                                </li>
                                <li class="environment dropdown mega-dropdown">
                                    <a href="/home/group?group=@News_Manage.Common.Country.Taiwan">Du học Đài</a>
                                </li>
                                <li class="world dropdown mega-cat-dropdown">
                                    <a href="/home/group?group=@News_Manage.Common.Country.Canada">Kết nối Canada</a>
                                </li>
                                <li class="technology dropdown mega-cat-dropdown">
                                    <a href="/home/group?group=@News_Manage.Common.Country.Germany">Du học Đức</a>
                                </li>
                                <li class="sports"><a href="/home/group?group=@News_Manage.Common.Country.Job.ToString()">Việc làm nước ngoài</a></li>
                            </ul>
                        </nav>
                        <div class="searchNlogin">
                            <ul>
                                <li class="search-icon"><i class="fa fa-search"></i></li>
                                <li class="dropdown user-panel">
                                    <a href="/user/login" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i></a>
                                    <div class="dropdown-menu top-user-section">
                                        <div class="top-user-form">
                                            <form id="top-login" role="form" action="/user/login" method="POST">
                                                @Html.AntiForgeryToken()
                                                <div class="input-group" id="top-login-username">
                                                    <span class="input-group-addon"><img src="~/Uploads/images/images/others/user-icon.png" alt="" /></span>
                                                    <input id="UserName" name="UserName" type="text" class="form-control" placeholder="Email" required="">
                                                </div>
                                                <div class="input-group" id="top-login-password">
                                                    <span class="input-group-addon"><img src="~/Uploads/images/images/others/password-icon.png" alt="" /></span>
                                                    <input id="Password" name="Password" type="password" class="form-control" placeholder="Password" required="">
                                                </div>
                                                <div>
                                                    <p class="reset-user">Forgot <a href="#">Password/Username?</a></p>
                                                    <button class="btn btn-danger" type="submit">Login</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                            <div class="search">
                                <form role="form" action="/home/search">
                                    <input name="search" id="search" type="text" class="search-form" autocomplete="off" placeholder="Search & Press Enter">
                                </form>
                            </div> <!--/.search-->
                        </div><!-- searchNlogin -->*@
                </div>
            </div>
        </header><!--/#navigation-->

        <div class="container">
            <div class="page-breadcrumbs">
                <h1 class="section-title">@Html.DisplayFor(p => p.tblGroup.Title)</h1>
            </div>
            <div class="section">
                <div class="row">
                    <div class="col-md-9 col-sm-8">
                        <div id="site-content" class="site-content">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="left-content">
                                        <div class="details-news">
                                            <div class="post">
                                                <div class="entry-header">
                                                    <h1>@Html.DisplayFor(p => p.Titles)</h1>
                                                    <div class="entry-thumbnail">
                                                        <img class="img-responsive" src="@Html.DisplayFor(p => p.Img)" alt="">
                                                    </div>
                                                </div>
                                                <div class="post-content">
                                                    <div class="entry-meta">
                                                        <ul class="list-inline">
                                                            <li class="posted-by"><i class="fa fa-user"></i> by <a href="#">Owen Williams</a></li>
                                                            <li class="publish-date"><a href="#"><i class="fa fa-clock-o"></i> @Html.DisplayFor(p => p.CreateDate) </a></li>
                                                            <li class="views"><a href="#"><i class="fa fa-eye"></i>@Html.DisplayFor(p => p.View)</a></li>
                                                            <li class="comments"><i class="fa fa-comment-o"></i><a href="#">@Html.DisplayFor(p => p.tblComments.Count)</a></li>
                                                        </ul>
                                                    </div>
                                                    <h2 class="entry-title">@Html.DisplayFor(p => p.Describe) </h2>
                                                    <div class="entry-content">
                                                        @Html.Raw(Model.Content)
                                                    </div>
                                                </div>
                                            </div><!--/post-->
                                        </div><!--/.section-->
                                    </div><!--/.left-content-->
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-4">
                        <div id="sitebar">
                            @Html.Partial("FollowUs")
                        </div><!--/#sitebar-->
                    </div>
                </div>
            </div><!--/.section-->
        </div><!--/.container-->

        <div class="footer-widget">
            @Html.Partial("../menu/newsfooterwidget")
        </div><!--/.footer-widget-->
    </div><!--/#main-wrapper-->

    <footer id="footer">
        @Html.Partial("../menu/newsfooter")
    </footer>
    @Scripts.Render("~/bundles/news")
</body>
</html>